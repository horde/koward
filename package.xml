        <file name="query.png" role="www" />
       </dir> <!-- /www/Koward/app/themes/graphics -->
       <dir name="kolab">
        <file name="screen.css" role="www" />
       </dir> <!-- /www/Koward/app/themes/kolab -->
      </dir> <!-- /www/Koward/app/themes -->
