   <package>
    <name>Horde_Framework</name>
    <channel>pear.horde.org</channel>
   </package>
   <package>
    <name>Kolab_Server</name>
    <channel>pear.horde.org</channel>
   </package>
   <package>
    <name>Routes</name>
    <channel>pear.horde.org</channel>
   </package>
   <package>
    <name>View</name>
    <channel>pear.horde.org</channel>
   </package>
  </required>
  <optional>
   <package>
    <name>PHPUnit</name>
    <channel>pear.phpunit.de</channel>
   </package>
  </optional>
